nums1 = [1,2,2,1]
nums2 = [2,2]

nums1 = [4,9,5]
nums2 = [9,4,9,8,4]

if len(nums2) > len(nums1):
  nums1, nums2 = nums2, nums1

hashmap = dict()
for num in nums1:
  if nums in hashmap:
    hashmap(num) += 1
  else:
    hashmap(num) = 1

output = list()
for num in nums2:
  if num in hashmap and hashmap(num) >= 1:
    hashmap(num) -= 1
    output.append(num)

print(output)
